-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- SETUP
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function debugLog(...)
    if DEBUG_MODE then
        print(...)
    end
end

local AvatarStorage = ReplicatedStorage:FindFirstChild("AvatarStorage")
if not AvatarStorage then
    AvatarStorage = Instance.new("Folder")
    AvatarStorage.Name = "AvatarStorage"
    AvatarStorage.Parent = ReplicatedStorage
end

local BoyFolder = AvatarStorage:FindFirstChild("Boy")
if not BoyFolder then
    BoyFolder = Instance.new("Folder")
    BoyFolder.Name = "Boy"
    BoyFolder.Parent = AvatarStorage
end

local GirlFolder = AvatarStorage:FindFirstChild("Girl")
if not GirlFolder then
    GirlFolder = Instance.new("Folder")
    GirlFolder.Name = "Girl"
    GirlFolder.Parent = AvatarStorage
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- LOADER FUNCTIONS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function getUserIdFromUsername(username, retries)
    retries = retries or 3
    for attempt = 1, retries do
        local success, userId = pcall(function()
            return Players:GetUserIdFromNameAsync(username)
        end)
        if success and userId then
            return userId
        end
        if attempt < retries then
            task.wait(0.5)
        end
    end
    return nil
end

local function loadAvatarData()
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    print("ğŸ”„ LOADING AVATAR DATA...")
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    
    local totalLoaded = 0
    local totalFailed = 0
    local startTime = os.clock()
    
    debugLog("ğŸ‘¦ Loading Boy avatars...")
    for i, username in ipairs(AvatarData.Boy) do
        local userId = getUserIdFromUsername(username)
        
        if userId then
            local value = Instance.new("IntValue")
            value.Name = username
            value.Value = userId
            value.Parent = BoyFolder
            totalLoaded = totalLoaded + 1
            debugLog("  âœ…", i .. ".", username, "â†’", userId)
        else
            totalFailed = totalFailed + 1
            warn("  âŒ", i .. ".", username, "FAILED")
        end
        
        task.wait(0.01)
    end
    
    debugLog("ğŸ‘§ Loading Girl avatars...")
    for i, username in ipairs(AvatarData.Girl) do
        local userId = getUserIdFromUsername(username)
        
        if userId then
            local value = Instance.new("IntValue")
            value.Name = username
            value.Value = userId
            value.Parent = GirlFolder
            totalLoaded = totalLoaded + 1
            debugLog("  âœ…", i .. ".", username, "â†’", userId)
        else
            totalFailed = totalFailed + 1
            warn("  âŒ", i .. ".", username, "FAILED")
        end
        
        task.wait(0.01)
    end
    
    local loadTime = math.floor((os.clock() - startTime) * 10) / 10
    
    local ReadyFlag = Instance.new("BoolValue")
    ReadyFlag.Name = "DataReady"
    ReadyFlag.Value = true
    ReadyFlag.Parent = AvatarStorage
    
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    print("âœ… AVATAR DATA LOADED!")
    print("ğŸ‘¤ Author: ItoRenz00")
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    print("ğŸ“Š Total Loaded:", totalLoaded)
    print("âŒ Total Failed:", totalFailed)
    print("â±ï¸  Load Time:", loadTime, "seconds")
    print("ğŸ“ Storage:", AvatarStorage:GetFullName())
    print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
    if not DEBUG_MODE then
        print("ğŸ’¡ Tip: Set DEBUG_MODE = true untuk detailed logs")
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- INITIALIZATION
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

task.spawn(function()
    loadAvatarData()
end)
